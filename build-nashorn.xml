<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project MuteGrammar">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="./MuteInterpreterNashorn.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="MuteInterpreter"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="./bin"/>
            <zipfileset excludes="META-INF/*.SF" src="./lib/antlr-4.1-complete.jar"/>
        	<zipfileset src="./lib/js.jar"/>
        	<zipfileset src="./lib/commons-lang3-3.3.2.jar"/>
        </jar>
    </target>
</project>
